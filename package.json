    "eslint": "7.32",
    "mocha": "^10.0.0",
    "mockery": "^2.1.0",
    "sinon": "^14.0.0",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "base64-js": "^1.5.1",
    "bignumber.js": "^9.1.0",
